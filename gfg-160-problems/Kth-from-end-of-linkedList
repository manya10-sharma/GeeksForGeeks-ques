/* Structure of node
class Node
{
    int data;
    Node next;
    Node(int d) {data = d; next = null; }
} */
class Solution {
    // Function to find the data of kth node from the end of a linked list.
    int getKthFromLast(Node head, int k) {
        
        Node temp = head;
        int count = 0;
        
        while(temp != null){
            temp = temp.next;
            count++;
        }
        
        temp = head;
        
        if(k > count){
            return -1;
        }
        
        int n = count - k;
        for(int i = 0 ; i<n ; i++){
            temp = temp.next;
        }
        return temp.data;
    }
}