/*
class Node {
    int data;
    Node next;

    Node(int x)
    {
        data = x;
        next = null;
    }
};
*/
class Solution {
    public int cycleStart(Node head) {
        if(head==null || head.next == null) return -1;
        Node slow = head;
        Node fast = head;
        boolean hasCycle = false;
        
        while(fast!=null && fast.next != null){
            slow = slow.next;
            fast = fast.next.next;
            if(slow == fast){
                hasCycle = true;
                 break;
            }
        }
        if(!hasCycle) return -1;
        
        Node temp = head;
        
        while(slow!=temp){
            slow = slow.next;
            temp = temp.next;
        }
        
        return temp.data;
    }
}