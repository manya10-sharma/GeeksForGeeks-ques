class Solution {
    public static char extraChar(String s1, String s2){
        int[] freq = new int[256];
        
        for(char ch : s1.toCharArray()){
            freq[ch]++;
        }
        
        for(char ch : s2.toCharArray()){
            freq[ch]--;
        }
        
        for(int i=0; i<256 ; i++){
            if(freq[i]<0){
                return (char)i;
            }
        }
        return ' ';
    }
}
