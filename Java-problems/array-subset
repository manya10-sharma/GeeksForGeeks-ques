
class Solution {
    public boolean isSubset(int a[], int b[]) {
        Arrays.sort(a);
        Arrays.sort(b);
        
        int i=0, j=0;
        
        while(i<a.length && j<b.length){
            if(a[i] == b[j]){
                i++;
                j++;
            }
            else if(a[i] < b[j]){
                i++;
            }
            else{
               return false;
            }
        }
        return (j==b.length);
    }
}
