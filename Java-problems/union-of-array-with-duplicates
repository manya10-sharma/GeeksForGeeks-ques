class Solution {
    public static ArrayList<Integer> findUnion(int[] a, int[] b) {
        HashSet<Integer> set = new HashSet<>();
        
        for(int n : a){
            set.add(n);
        }
        for(int n : b){
            set.add(n);
        }
        ArrayList<Integer> unionList = new ArrayList<>(set);
        Collections.sort(unionList);
        
        return unionList;
    }
}